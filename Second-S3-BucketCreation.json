{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "S3-Core-Prsm-BucketCreation",
    "Parameters": {
        "RawBucket1": {
            "Type": "String",
            "Default": "poc-bucket1-raw122",
            "Description": "Enter Prism Bucket Name"
        },
	"PrasedBucket2": {
            "Type": "String",
            "Default": "poc-bucket1-parsed1233",
            "Description": "Enter Prism Bucket Name"
        }	
    },
    "Resources": {
        "S3RawBucket": {
            "Type": "AWS::S3::Bucket",
            "Properties": {
                "BucketName": {
                    "Ref": "RawBucket1"
                }
	    }
	},
 	"S3ParsedBucket": {
            "Type": "AWS::S3::Bucket",
            "Properties": {
                "BucketName": {
                    "Ref": "PrasedBucket2"
                }
	    }
	}
    }
}
